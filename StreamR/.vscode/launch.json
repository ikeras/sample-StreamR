{
	"version": "0.2.1",
	"configurations": [
		{
			"name": ".NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"program": "${workspaceFolder}/StreamR.App/bin/Debug/netcoreapp3.0/StreamR.exe",
			"args": [],
			"cwd": "${workspaceFolder}/StreamR.App",
			"env": {				
				"ASPNETCORE_ENVIRONMENT": "Development"
			},
			"stopAtEntry": false,
			"launchBrowser": {
				"enabled": true,
				"args": "${auto-detect-url}",
				"windows": {
					"command": "cmd.exe",
					"args": "/C start ${auto-detect-url}"
				}
			}
		}
	]
}